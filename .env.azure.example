# Azure Deployment Environment Variables

# Copy this file to .env.azure and fill in the values
# DO NOT commit .env.azure to version control

# Azure Environment
AZURE_ENV_NAME=hivecraft-prod
AZURE_LOCATION=eastus2

# PostgreSQL Admin Credentials
POSTGRES_ADMIN_PASSWORD=your-strong-password-here

# Session Secret (generate with: openssl rand -base64 32)
SESSION_SECRET=your-session-secret-here

# Replit OAuth (if using Replit Auth)
REPLIT_CLIENT_ID=your-replit-client-id
REPLIT_CLIENT_SECRET=your-replit-client-secret
